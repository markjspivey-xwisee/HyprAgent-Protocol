    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hyprcat/protocol": "workspace:*",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "compression": "^1.8.0",
    "express-rate-limit": "^7.5.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^10.0.0",
    "typescript": "~5.8.2",
    "tsx": "^4.19.0"
  },
  "license": "Apache-2.0"
}
