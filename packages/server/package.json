{
  "name": "@hyprcat/server",
  "version": "1.0.0",
  "description": "HyprCAT Gateway Server - serves HyprCAT-compliant resources with governance and provenance",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "start": "node dist/index.js",
